{>"layouts/default" /}

{<content}
	<a href="/i"><h4>Interviews</h4></a>
	{#interviews}	
		<a href='/i/{id}'>
			<div class="clearfix" style="padding:10px 0; border-bottom: solid 1px #eee;">
				<span class="pull-left">{name} - {position}</span>
			</div>
		</a>
	{:else}
		<p>No interviews currently available.</p>
	{/interviews}

	<br>

	<a href="/a"><h4>Articles</h4></a>
	{#articles}	
		<a href='/a/{aid}'>
			<div class="clearfix" style="padding:10px 0; border-bottom: solid 1px #eee;">
				<span class="pull-left">{title}  <i>{subtitle}</i></span>
				<span class="pull-right">{timestamp}</span>
			</div>
		</a>
	{:else}
		<p>No articles currently available.</p>
	{/articles}
	
{/content}
